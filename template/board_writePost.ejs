<form action="/uploadPost" method="POST">
    <table>
        <tr>
            <td>
                <input id="post_title" name="post_title" type="text" placeholder="제목">
            </td>
        </tr>
        <tr>
            <td>
                <textarea id="post_content" name="post_content" placeholder="내용" cols="100" rows="20"></textarea>
            </td>
        </tr>
    </table>
    <button type="submit ">Upload</button>
</form>