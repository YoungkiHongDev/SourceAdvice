<form action="/uploadPost" method="POST">
    <table>
        <tr>
            <td>
                <input id="post_title" name="post_title" type="text" placeholder="제목">
            </td>
        </tr>
        <tr>
            <td>
                <label>언어 선택</label>
                <label><input id="post_kategorie"name="post_kategorie" type="radio" value="C">C</label>
                <label><input id="post_kategorie"name="post_kategorie" type="radio" value="Java">Java</label>
                <label><input id="post_kategorie"name="post_kategorie" type="radio" value="Python">Python</label>
            </td>
        </tr>
        <tr>
            <td>
                <textarea id="post_content" name="post_content" placeholder="내용" cols="100" rows="20"></textarea>
            </td>
        </tr>
    </table>
    <button class="btn btn-outline-primary" type="submit ">글 쓰기</button>
    <form action="/BackHome" method="POST">
        <button class="btn btn-outline-secondary" type="submit">글 목록</button>
    </form>
</form>